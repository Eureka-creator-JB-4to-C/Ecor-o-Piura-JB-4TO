<script>
  // Quiz
  function checkAnswer(answer) {
    const result = document.getElementById('quiz-result');
    if(answer === 'a') {
      result.textContent = "✅ Correcto. Los residuos sólidos y aguas residuales son una de las principales causas.";
    } else {
      result.textContent = "❌ Incorrecto. Intenta nuevamente.";
    }
  }

  // Juego de memoria
  const grid = document.getElementById('memory-grid');
  const cardsArray = ['🌳','🌊','🐟','🗑️','🌳','🌊','🐟','🗑️'];
  let firstCard = null;
  let lock = false;

  function shuffle(array) {
    return array.sort(() => Math.random() - 0.5);
  }

  function createBoard() {
    const shuffled = shuffle(cardsArray);
    shuffled.forEach(symbol => {
      const card = document.createElement('div');
      card.classList.add('card');
      card.dataset.symbol = symbol;
      card.textContent = "?";
      card.addEventListener('click', flipCard);
      grid.appendChild(card);
    });
  }

  function flipCard() {
    if (lock) return;
    if (this.classList.contains('flipped')) return;

    this.classList.add('flipped');
    this.textContent = this.dataset.symbol;

    if (!firstCard) {
      firstCard = this;
      return;
    }

    if (this.dataset.symbol === firstCard.dataset.symbol) {
      firstCard = null;
    } else {
      lock = true;
      setTimeout(() => {
        this.classList.remove('flipped');
        this.textContent = "?";
        firstCard.classList.remove('flipped');
        firstCard.textContent = "?";
        firstCard = null;
        lock = false;
      }, 1000);
    }
  }

  createBoard();
</script>
